{
	// "extends": "@tsconfig/node16/tsconfig.json",
	"compileOnSave": false,
	"compilerOptions": {
		"module": "CommonJS",
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"pretty": true,
		"moduleResolution": "Node",
		"target": "es5",
		"downlevelIteration": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"noEmitOnError": false,
		"experimentalDecorators": true,
		"noUnusedParameters": false,
		"noUnusedLocals": false,
		"importHelpers": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"lib": ["dom", "dom.iterable", "es2015", "es2016", "es2017", "es2018", "es2019", "es2020", "es2021", "es2022", "esnext.asynciterable"],
		"typeRoots": ["./node_modules/@types"],
		"outDir": "./dist",
		"baseUrl": ".",
		"rootDir": "src"
	},
	"include": ["./src/**/*.ts", "./src/**/*.tsx"],
	"exclude": ["**/*.test.ts", "**/*.test.tsx", "**/test/*", "**/__tests__/*"]
}
